@model IEnumerable<Lesson04.Models.DataModels.Member> // định kiểu
    ViewData["Title"] = "ListModel";
}

<h1>ListModel</h1>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Member Id</th>
            <th>FullName</th>
            <th>UserName</th>
            <th>Password</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.MemberId</td>
                <td>@item.FullName</td>
                <td>@item.UserName</td>
                <td>@item.Password</td>
                <td>@item.Email</td>
            </tr>
        }
    </tbody>
</table>
